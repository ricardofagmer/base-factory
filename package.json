{
  "name": "code",
  "version": "0.0.1",
  "main": "app.ts",
  "description": "Awesome project developed with TypeORM.",
  "scripts": {
    "start": "nodemon tsc && node dist/app.ts",
    "dev": "nodemon ./src/app.ts",
    "npx": "npx ts-node ./src/app.ts",
    "migrations": "ts-node --transpile-only ./node_modules/typeorm/cli.js migration:run",
    "create-migration": "typeorm migration:create -n PostRefactoring",
    "revert-migration": "typeorm migration:revert",
    "test": "jest"
  },
  "dependencies": {
    "body-parser": "^1.19.0",
    "express": "^4.5.0",
    "mysql2": "^2.3.2",
    "reflect-metadata": "^0.1.13",
    "supertest": "^6.1.6",
    "ts-node-dev": "^1.1.8",
    "typeorm": "0.2.38",
    "typescript": "^4.4.3",
    "yarn": "^1.22.17"
  },
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@types/jest": "^27.0.3",
    "@types/node": "^16.11.6",
    "jest": "^27.3.1",
    "nodemon": "^2.0.13",
    "ts-node": "^10.3.1",
    "typescript": "3.3.3333"
  }
}
